<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="$(SolutionDir)Asv.Drones.Gui.Custom.props"/>
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Asv.Cfg" Version="1.12.7" />
      <PackageReference Include="Asv.Common" Version="1.12.7" />
      <PackageReference Include="Avalonia" Version="11.0.0" />
      <PackageReference Include="Avalonia.ReactiveUI" Version="11.0.0" />
      <PackageReference Include="Avalonia.Xaml.Interactions" Version="11.0.0" />
      <PackageReference Include="FluentAvaloniaUI" Version="2.0.0" />
      <PackageReference Include="Material.Icons.Avalonia" Version="2.0.1" />
      <PackageReference Include="ReactiveUI.Fody" Version="19.4.1" />
      <PackageReference Include="ReactiveUI.Validation" Version="3.1.7" />
      <PackageReference Include="System.ComponentModel.Composition" Version="7.0.0" />
    </ItemGroup>




    <ItemGroup>
      <EmbeddedResource Update="RS.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>RS.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="RS.ru.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>RS.ru.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>



    <ItemGroup>
      <Compile Update="RS.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>RS.resx</DependentUpon>
      </Compile>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\asv-drones\src\Asv.Drones.Gui.Core\Asv.Drones.Gui.Core.csproj" />
        <ProjectReference Include="..\..\asv-drones\src\Asv.Drones.Gui.Map\Asv.Drones.Gui.Map.csproj" />
    </ItemGroup>

</Project>
